{
  "expo": {
    "name": "gabinete-perinatal",
    "slug": "gabinete-perinatal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/IconApp2.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/IconApp2.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff",
      "imageSize": [
        500,
        500
      ]
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/IconApp2.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/IconApp2.png"
    },
    "plugins": [
      "expo-sqlite"
    ]
  }
}
